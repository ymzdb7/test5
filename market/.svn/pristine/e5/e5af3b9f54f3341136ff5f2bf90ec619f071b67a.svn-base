 <div style="margin-left: 30px"><h3>商品详情：</h3></div>  
   <table   style="margin-left: 70px ">
	<tr>
	<th width="300">商品名称</th>
	<th width="300">商品价格</th>
	<th width="300">下单数量</th>
	<th width="300">库存数量</th>
	</tr>

  		 <template v-for="item in goods"  v-if="goods !=null" > 
  			
  					<tr>
					<td style="padding-left:10px">{{item.goodsName}}</td> 
					<td style="padding-left:15px">{{item.goodsPrice}}元</td>
					<td style="padding-left:25px">{{item.count}}</td> 
					<td style="padding-left:25px">{{item.counts}}</td>
					 </tr>
					 
		 </template>
 
 </table>
   
   
   
